{
  "name": "surrial",
  "version": "0.0.0",
  "description": "Serialize anything. This is surreal!",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "clean": "rimraf \"+(src|test)/**/+(*.map|*.js|*.d.ts)\"",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "tslint --project .",
    "pretest": "npm run build",
    "test": "nyc --check-coverage --reporter=html --report-dir=reports/coverage --lines 80 --functions 80 --branches 75 mocha \"test/**/*.js\"",
    "posttest": "stryker run"
  },
  "keywords": [
    "serialize",
    "deserialize",
    "marshal",
    "marshalling",
    "classes"
  ],
  "author": "Nico Jansen",
  "license": "Apache2",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.2",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "stryker": "^0.19.2",
    "stryker-api": "^0.13.0",
    "stryker-html-reporter": "^0.12.1",
    "stryker-mocha-framework": "^0.8.3",
    "stryker-mocha-runner": "^0.10.5",
    "stryker-typescript": "^0.9.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
